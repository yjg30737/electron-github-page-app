{
  "name": "electron-github-page-app",
  "version": "0.0.1",
  "description": "Showing Github Page with Electron app",
  "main": "main.js",
  "author": "Jung Gyu Yoon",
  "license": "MIT",
  "scripts": {
    "start": "electron .",
    "build:osx": "electron-builder --mac",
    "build:linux": "npm run build:linux32 && npm run build:linux64",
    "build:linux32": "electron-builder --linux --ia32",
    "build:linux64": "electron-builder --linux --x64",
    "build:win": "npm run build:win32 && npm run build:win64",
    "build:win32": "electron-builder --win portable --ia32",
    "build:win64": "electron-builder --win portable --x64"
  },
  "repository": "https://github.com/yjg30737/electron-github-page-app",
  "keywords": [
    "Electron",
    "githubpage",
    "start",
    "webapp",
    "example",
    "tutorial",
    "demo"
  ],
  "devDependencies": {
    "electron": "^19.0.0",
    "electron-builder": "^23.6.0"
  },
  "dependencies": {
    "package.json": "^2.0.1",
    "shell": "^0.9.4"
  }
}
